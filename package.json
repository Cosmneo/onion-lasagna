{
  "name": "@cosmneo/onion-lasagna",
  "version": "0.1.0",
  "type": "module",
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "eslint": "^9.39.2",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.5.0",
    "prettier": "^3.7.4",
    "tsup": "^8.5.1",
    "typescript-eslint": "^8.50.1",
    "arktype": "^2.1.29",
    "zod": "^4.2.1"
  },
  "peerDependencies": {
    "@sinclair/typebox": "^0.34.41",
    "arktype": "^2.1.29",
    "typescript": "^5",
    "valibot": "^1.2.0",
    "zod": "^4.2.1"
  },
  "dependencies": {
    "uuid": "^13.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "exports": {
    "./backend/core/bounded-context": {
      "types": "./dist/backend/core/bounded-context.d.ts",
      "import": "./dist/backend/core/bounded-context.js",
      "require": "./dist/backend/core/bounded-context.cjs",
      "default": "./dist/backend/core/bounded-context.js"
    },
    "./backend/core/global": {
      "types": "./dist/backend/core/global.d.ts",
      "import": "./dist/backend/core/global.js",
      "require": "./dist/backend/core/global.cjs",
      "default": "./dist/backend/core/global.js"
    },
    "./backend/core/validators/zod": {
      "types": "./dist/backend/core/validators/zod.d.ts",
      "import": "./dist/backend/core/validators/zod.js",
      "require": "./dist/backend/core/validators/zod.cjs",
      "default": "./dist/backend/core/validators/zod.js"
    },
    "./backend/core/validators/arktype": {
      "types": "./dist/backend/core/validators/arktype.d.ts",
      "import": "./dist/backend/core/validators/arktype.js",
      "require": "./dist/backend/core/validators/arktype.cjs",
      "default": "./dist/backend/core/validators/arktype.js"
    }
  },
  "packageManager": "bun",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  }

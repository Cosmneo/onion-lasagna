{
  "name": "@cosmneo/onion-lasagna",
  "version": "0.2.1",
  "description": "Enterprise-grade TypeScript library for building backend applications with hexagonal/onion architecture",
  "type": "module",
  "license": "MIT",
  "author": "Cosmneo",
  "homepage": "https://onion-lasagna.cosmneo.com",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cosmneo/onion-lasagna.git",
    "directory": "packages/onion-lasagna"
  },
  "bugs": {
    "url": "https://github.com/Cosmneo/onion-lasagna/issues"
  },
  "keywords": [
    "hexagonal-architecture",
    "onion-architecture",
    "clean-architecture",
    "ddd",
    "domain-driven-design",
    "typescript",
    "backend",
    "enterprise",
    "value-objects",
    "entities",
    "aggregates",
    "use-cases",
    "ports-and-adapters"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/coverage-v8": "^4.0.16",
    "tsup": "^8.5.1",
    "uuid": "^13.0.0",
    "vitest": "^4.0.16",
    "zod": "4.3.6"
  },
  "peerDependencies": {
    "typescript": "^5",
    "uuid": "^13.0.0"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "prepublishOnly": "bun run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "default": "./dist/index.js"
    },
    "./global": {
      "types": "./dist/global.d.ts",
      "import": "./dist/global.js",
      "require": "./dist/global.cjs",
      "default": "./dist/global.js"
    },
    "./ports": {
      "types": "./dist/ports.d.ts",
      "import": "./dist/ports.js",
      "require": "./dist/ports.cjs",
      "default": "./dist/ports.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs",
      "default": "./dist/types.js"
    },
    "./http": {
      "types": "./dist/http/index.d.ts",
      "import": "./dist/http/index.js",
      "require": "./dist/http/index.cjs",
      "default": "./dist/http/index.js"
    },
    "./http/schema": {
      "types": "./dist/http/schema/index.d.ts",
      "import": "./dist/http/schema/index.js",
      "require": "./dist/http/schema/index.cjs",
      "default": "./dist/http/schema/index.js"
    },
    "./http/schema/types": {
      "types": "./dist/http/schema/types.d.ts",
      "import": "./dist/http/schema/types.js",
      "require": "./dist/http/schema/types.cjs",
      "default": "./dist/http/schema/types.js"
    },
    "./http/route": {
      "types": "./dist/http/route/index.d.ts",
      "import": "./dist/http/route/index.js",
      "require": "./dist/http/route/index.cjs",
      "default": "./dist/http/route/index.js"
    },
    "./http/server": {
      "types": "./dist/http/server/index.d.ts",
      "import": "./dist/http/server/index.js",
      "require": "./dist/http/server/index.cjs",
      "default": "./dist/http/server/index.js"
    },
    "./http/shared": {
      "types": "./dist/http/shared/index.d.ts",
      "import": "./dist/http/shared/index.js",
      "require": "./dist/http/shared/index.cjs",
      "default": "./dist/http/shared/index.js"
    },
    "./http/openapi": {
      "types": "./dist/http/openapi/index.d.ts",
      "import": "./dist/http/openapi/index.js",
      "require": "./dist/http/openapi/index.cjs",
      "default": "./dist/http/openapi/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
